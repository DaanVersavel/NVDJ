extends layout

block content
  header#home.w3-display-container.w3-content.w3-wide(style='max-width:100%;')
    img.w3-image(src='images/Foto%202.jpg' alt='Foto publiek + logo NVDJ' width=auto height=auto)
  .bodycountdown.w3-text-white
    h5 Countdown tot NVDJ XV
    #countdown.countdown
      .time
        h6#days
        small DAGEN
      .time
        h6#hours
        small UREN
      .time
        h6#minutes
        small MINUTEN
      .time
        h6#seconds
        small SECONDEN
    script(src='/javascripts/countdown.js')
